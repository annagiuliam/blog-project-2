{
  "name": "blog-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "gettext-extract" : "gettext-extract",
    "extract-gettext-cli": "gettext-extract --attribute v-translate --output dictionary.pot $(find ./src/views -type f -name '*.vue')",
    "xtract" : "./node_modules/.bin/gettext-extract --output dictionary.pot `find src -name '*.js' -o -name '*.vue'`"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "eslint-plugin-standard": "^5.0.0",
    "gettext-extract": "^2.0.1",
    "vue": "^2.6.11",
    "vue-gettext": "^2.1.12",
    "vue-router": "^3.5.3",
    "vuetify": "^2.4.0",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@mdi/font": "^6.5.95",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.15",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "easygettext": "^2.17.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "material-design-icons-iconfont": "^6.1.1",
    "prettier": "^2.2.1",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "vue-cli-plugin-vuetify": "~2.4.4",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended",
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "no-console": 0,
      "vue/max-attributes-per-line": "warn",
      "vue/require-prop-types": "off",
      "vue/multi-word-component-names": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
